{
 "name": "@payfunc/template-library",
 "version": "0.0.0",
 "description": "Template for NPM library",
 "author": "PayFunc",
 "license": "MIT",
 "private": false,
 "repository": "https://github.com/payfunc/template-library",
 "main": "dist/index.js",
 "typings": "dist/index.d.ts",
 "git": {
  "tagName": "v${version}"
 },
 "github": {
  "release": true
 },
 "jest": {
  "globals": {
   "ts-jest": {
    "tsConfig": "tsconfig.test.json"
   }
  },
  "testEnvironment": "node",
  "testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
  "testPathIgnorePatterns": [
   "node_modules/",
   "dist/"
  ],
  "collectCoverageFrom": [
   "**/*.{ts,tsx,js,jsx}",
   "!**/node_modules/**",
   "!**/dist/**"
  ],
  "preset": "ts-jest",
  "testMatch": null
 },
 "scripts": {
  "dev": "watch 'npm run build' .",
  "lint": "tslint --config tslint.json --project .",
  "build": "tsc -p .",
  "test": "jest",
  "test:watch": "watch jest",
  "prepare": "npm run build",
  "clean": "rm -rf dist node_modules coverage"
 },
 "dependencies": {},
 "devDependencies": {
  "@types/jest": "^24.0.17",
  "@types/node": "^12.6.9",
  "babel-core": "^6.26.3",
  "babel-jest": "^24.8.0",
  "jest": "^24",
  "ts-jest": "^24.0.2",
  "tslint": "^5.20.0",
  "typescript": "^3.5.3",
  "watch": "^1.0.2"
 }
}
